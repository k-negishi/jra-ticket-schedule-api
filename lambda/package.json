{
  "name": "jra-calendar-api",
  "version": "1.0.0",
  "description": "JRA Calendar API Lambda Function",
  "main": "src/main.py",
  "scripts": {
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "logs": "serverless logs -f getCalendarEvents",
    "invoke:local": "serverless invoke local --function getCalendarEvents --data '{\"queryStringParameters\": {\"year\": \"2024\", \"month\": \"1\", \"day\": \"15\"}}'",
    "test": "./venv/bin/python -m pytest tests/ -v",
    "setup:dev": "python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "activate:dev": "source venv/bin/activate"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-python-requirements": "^6.1.2"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "google-calendar",
    "python"
  ],
  "author": "",
  "license": "MIT"
}
